<h1>Listing feeds</h1>

<table>
  <thead>
    <tr>
      <th>Fid</th>
      <th>Name</th>
      <th>Url</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feeds.each do |feed| %>
    <tr>
      <td><%= feed.fid %></td>
      <td><%= feed.name %></td>
      <td><%= feed.url %></td>
      <td><%= feed.category %></td>
      <td><%= link_to 'Show', feed %></td>
      <!-- <td><%#= link_to 'Edit', edit_feed_path(feed) %></td> -->
      <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feed', new_feed_path %>
